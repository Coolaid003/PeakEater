    "[python]": {
        "editor.defaultFormatter": "ms-python.black-formatter"
    },
    "python.languageServer": "Pylance",
    "python.analysis.extraPaths": [
        "/usr/local/Cellar/conan/2.0.3/libexec/lib/python3.11/site-packages"
    ],
    "python.formatting.provider": "none",
    "python.formatting.blackArgs": [
        "--line-length",
        "180"
    ],
    "[cpp]": {
        "editor.wordBasedSuggestions": false,
        "editor.suggest.insertMode": "replace",
        "editor.semanticHighlighting.enabled": true,
        "editor.defaultFormatter": "ms-vscode.cpptools"
    },
    "C_Cpp.formatting": "clangFormat",
    "C_Cpp.clang_format_style": "file:${workspaceFolder}/config/.clang-format",
    // Extensions settings
    "taskExplorer.enabledTasks": {
        "ant": false,
        "bash": false,
        "composer": false,
        "gradle": false,
        "grunt": false,
        "gulp": false,
        "make": false,
        "maven": false,
        "npm": false,
        "perl": false,
        "pipenv": false,
        "powershell": false,
        "python": false,
        "ruby": false,
        "tsc": false
    },
    "taskExplorer.enableSideBar": true,
    "taskExplorer.groupMaxLevel": 5,
    "taskExplorer.groupWithSeparator": true,
    "taskExplorer.groupSeparator": ":",
    "grammarly.files.include": [
        "**/README.md",
        "**/readme.md"
    ],
